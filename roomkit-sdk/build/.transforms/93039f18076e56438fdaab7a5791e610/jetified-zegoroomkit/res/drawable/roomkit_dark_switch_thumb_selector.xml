<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_enabled="true">
        <shape android:shape="oval">
            <solid android:color="@color/roomkit_color_gray_level26" />
            <size android:width="19dp" android:height="19dp" />
            <stroke android:width="1dp" android:color="@color/roomkit_color_gray_level26" />
        </shape>
    </item>

    <item android:state_checked="false" android:state_enabled="true">
        <shape android:shape="oval">
            <solid android:color="@color/roomkit_color_gray_level26" />
            <size android:width="19dp" android:height="19dp" />
            <stroke android:width="1dp" android:color="@color/roomkit_color_gray_level26" />
        </shape>
    </item>

</selector>