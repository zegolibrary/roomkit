<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="34dp"
    android:layout_height="34dp"
    android:clickable="true">

    <im.zego.roomkit.widget.CircleImageView
        android:id="@+id/item_style_color"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_gravity="center"
        app:civ_border_color="@color/roomkit_color_gray_level24"
        app:civ_border_width="2dp"
        app:civ_circle_background_color="#ff0000"
        app:srcCompat="@android:color/transparent" />

    <ImageView
        android:id="@+id/item_style_ticker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/roomkit_toolbar_icon_selectcolor_white2" />
</FrameLayout>
