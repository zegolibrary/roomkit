    <style name="Roomkit_AppDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/roomkit_color_main_level1</item>
        <item name="colorPrimaryDark">@color/roomkit_color_main_level1</item>
        <item name="colorAccent">@color/roomkit_color_main_level2</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="Roomkit_AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/roomkit_color_main_level1</item>
        <item name="colorPrimaryDark">@color/roomkit_color_main_level1</item>
        <item name="colorAccent">@color/roomkit_color_main_level2</item>
