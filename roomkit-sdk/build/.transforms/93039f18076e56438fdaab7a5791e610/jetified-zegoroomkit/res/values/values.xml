        <item name="android:windowExitAnimation">@anim/roomkit_bottom_dialog_out</item>
    </style>
    <style name="Roomkit_BottomDialogFragmentStyle" parent="Roomkit_DialogFragmentStyle">
        <item name="android:windowAnimationStyle">@style/Roomkit_BottomDialogAnimation</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.2</item>
    </style>
    <style name="Roomkit_BottomDialogStyle" parent="android:style/Theme.Dialog">

        <item name="android:windowFullscreen">true</item>

        <item name="android:windowFrame">@null</item>
        <!--边框-->
        <item name="android:windowIsFloating">true</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item>
        <!--半透明-->
        <item name="android:windowNoTitle">true</item>
        <!--无标题-->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--背景透明-->
        <item name="android:backgroundDimEnabled">true</item>
        <!--模糊-->
    </style>
    <style name="Roomkit_DialogFragmentStyle" parent="Roomkit_AppDialogTheme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="Roomkit_MiguDialogStyle" parent="Roomkit_AppDialogTheme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
